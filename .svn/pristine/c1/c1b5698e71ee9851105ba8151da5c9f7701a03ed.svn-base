#searchResultOutput ul li{cursor:pointer; line-height:30px; padding-left:10px;}
#searchResultOutput ul li a {text-decoration:none;width:200px;margin-right:1px;display:inline-block;}
#searchResultOutput ul li button {border:1px solid #ccc;background-color:transparent;}
#searchResultOutput ul li:hover{background-color:#ddd;}
#searchResultOutput ul li span{  width:200px; color:#000; display:inline-block;}